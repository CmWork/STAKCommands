      <spirent.methodology.VerifyDbQueryCommand id="2121" 
       SqlQuery="SELECT  (CASE WHEN Tag.Name LIKE &apos;Net10%&apos; THEN
&apos;Blocking IP Address&apos; WHEN Tag.Name LIKE &apos;FragOff%&apos; THEN
&apos;Fragment Offset Not Zero&apos; WHEN Tag.Name LIKE &apos;FragMF%&apos;
THEN &apos;Fragment Flag Is Set&apos; ELSE &apos;Pass Through Stream&apos; END)
AS &apos;Stream Type&apos;, (Rx.FrameCount - Rx.DroppedFrameCount) as
&apos;Total Blocking Frames Passed Through&apos; FROM RxEotStreamResults AS Rx
JOIN TxEotStreamResults AS Tx JOIN RelationTable AS Rt JOIN Tag ON Rx.Comp32 =
Tx.StreamId AND Tx.ParentStreamBlock = Rt.SourceHnd AND Rt.TargetHnd =
Tag.Handle WHERE  (Rx.FrameCount - Rx.DroppedFrameCount)  &gt; 0 AND Rt.Type =
&apos;UserTag&apos; AND Tag.Name IN (&apos;Net10_ttStreamBlock&apos;,
&apos;FragOff_ttStreamBlock&apos;, &apos;FragMF_ttStreamBlock&apos;)"
       OperationType="EQUAL"

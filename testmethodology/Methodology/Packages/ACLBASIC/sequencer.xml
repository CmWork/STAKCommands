      <spirent.methodology.VerifyDbQueryCommand id="2123" 
       SqlQuery="SELECT (SELECT Rx.FrameCount as &apos;Frames&apos; FROM
RxEotStreamResults AS Rx JOIN TxEotStreamResults AS Tx JOIN RelationTable AS Rt
JOIN Tag ON Rx.Comp32 = Tx.StreamId AND Tx.ParentStreamBlock = Rt.SourceHnd AND
Rt.TargetHnd = Tag.Handle WHERE Rt.Type = &apos;UserTag&apos; AND Tag.Name =
&apos;Good_ttStreamBlock&apos;) AS &apos;Pass Through Packet Count&apos;,
(SELECT Rx.FrameCount as &apos;Bad Frames Passed Through&apos; FROM
RxEotStreamResults AS Rx JOIN TxEotStreamResults AS Tx JOIN RelationTable AS Rt
JOIN Tag ON Rx.Comp32 = Tx.StreamId AND Tx.ParentStreamBlock = Rt.SourceHnd AND
Rt.TargetHnd = Tag.Handle WHERE Rt.Type = &apos;UserTag&apos; AND Tag.Name IN
(&apos;FragOff_ttStreamBlock&apos;)) AS &apos;Fragment Offset Not Zero Packet
Count&apos;, (SELECT Rx.FrameCount as &apos;Bad Frames Passed Through&apos;
FROM RxEotStreamResults AS Rx JOIN TxEotStreamResults AS Tx JOIN RelationTable
AS Rt JOIN Tag ON Rx.Comp32 = Tx.StreamId AND Tx.ParentStreamBlock =
Rt.SourceHnd AND Rt.TargetHnd = Tag.Handle WHERE Rt.Type = &apos;UserTag&apos;
AND Tag.Name IN (&apos;FragMF_ttStreamBlock&apos;)) AS &apos;Fragment Flag Is
Set Packet Count&apos;, (SELECT Rx.FrameCount as &apos;Bad Frames Passed
Through&apos; FROM RxEotStreamResults AS Rx JOIN TxEotStreamResults AS Tx JOIN
RelationTable AS Rt JOIN Tag ON Rx.Comp32 = Tx.StreamId AND
Tx.ParentStreamBlock = Rt.SourceHnd AND Rt.TargetHnd = Tag.Handle WHERE Rt.Type
= &apos;UserTag&apos; AND Tag.Name IN (&apos;Net10_ttStreamBlock&apos;)) AS
&apos;Blocking IP Address Packet Count&apos;"
       OperationType="GREATER_THAN"
       RowCount="0"
       MinRowCount="0"
       MaxRowCount="0"
       UseMultipleResultsDatabases="FALSE"
       UseSummary="TRUE"
